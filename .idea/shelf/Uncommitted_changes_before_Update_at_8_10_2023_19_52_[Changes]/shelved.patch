+@import url('https://fonts.googleapis.com/css2?family=Archivo+Black&family=Asap&family=Bai+Jamjuree:wght@500&family=Roboto:wght@400;900&display=swap');
+* {
+    margin: 0;
+    padding: 0;
+    font-family: 'Bai Jamjuree', sans-serif;
+}
+
+:root {
+    --violeta-uno: #1F1E40;
+    --violeta-dos: #2A2C59;
+    --violeta-tres: #403BBF;
+    --violeta-cuatro: #9048D9;
+    --violeta-cinco: #BDC0F2;
+    --degradado-violeta: linear-gradient(5deg, #9148d993 0, #2A2C59 100%);;
+}
+
+body {
+    width: 100%;
+    height: 100vh;
+    max-height: 100vh;
+    overflow: hidden;
+}
+
+header {
+    flex-direction: row;
+    justify-content: space-between;
+    padding: 0 100px ;
+    align-items: center;
+    height: 10%;
+    width: 100%;
+    background-color: var(--violeta-dos);
+
+}
+
+main {
+    background-color: var(--violeta-uno);
+    background-image: url(../fondo_imagen.png);
+    background-size: cover;
+    background-position: center;
+    width: 100%;
+    height: 90%;
+}
+
+form {
+    display: flex;
+    flex-direction: column;
+    align-items: center;
+    gap: 20px;
+    background: var(--degradado-violeta);
+    width: 50%;
+    padding: 50px 100px;
+    border-radius: 6px;
+    margin: auto;
+    position: absolute;
+    left: 27%;
+    top: 25%;
+    color: white;
+}
+
+form > div{
+    width: 100%;
+}
+
+.infoBasica{
+    display: flex;
+    gap: 20px;
+    align-items: center;
+}
+
+#infoBasica_div1{
+    width: 150px;
+    height: 150px;
+}
+
+#editarImagen{
+    position: relative;
+    bottom: 150px;
+    text-align: center;
+    background-color: #00000024;
+    width: 100%;
+    height: 100%;
+    padding: 10px;
+    border-radius: 5px;
+}
+#editarImagen:hover{
+    background-color: #00000059;
+}
+
+#editarImagen:hover > img{
+    display: block;
+}
+
+
+#imagen_enviada{
+    width: 150px;
+    height: 100%;
+    object-fit: cover;
+    object-position: center;
+}
+
+/*BOTONES*/
+a.boton, button.boton{
+    background-color: var(--violeta-cuatro);
+    width: 200px;
+    font-size: 18px;
+    color: white;
+    font-weight: 500;
+}
+
+.boton:hover{
+    scale: 1.02;
+    color: white;
+    background-color: #763cb0 ;
+}
+
+a.boton_secundario, button.boton_secundario{
+    border: 3px solid var(--violeta-cuatro);
+    background-color: rgba(0, 0, 0, 0.127);
+    font-size: 18px;
+    width: 200px;
+    color: var(--violeta-cuatro);
+    font-weight: 500;
+}
+
+.boton_secundario:hover {
+    scale: 1.03;
+    color: white;
+}
+
+h2{
+    text-align: center;
+}
+
+.logo{
+    height: 100%;
+}
+
+
+
